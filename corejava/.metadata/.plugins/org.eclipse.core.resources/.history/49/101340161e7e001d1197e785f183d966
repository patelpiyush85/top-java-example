
class Parent
{
	void Parent1()
	{
		System.out.println("This is a Parent class");
	}
}
class Child extends Parent
{
	void Child1()
	{
		System.out.println("This is a Parent class");
	}
}
public class Inheritance {

	public static void main(String[] args) {
		Parent p=new Parent();
		p.Parent1();s
	}

}
