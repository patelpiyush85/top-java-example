
public class Starpettern7 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int i,j;
		for(i=0;i<=5;i++)
		{
			for(j=0;j<=1;j++)
			{
				if(i>=2 && j<=i+1)
				{
					System.out.print(" ");
					
				}
				else
				{
					System.out.print("*");
				}
				System.out.println();
			}
		}

	}

}
