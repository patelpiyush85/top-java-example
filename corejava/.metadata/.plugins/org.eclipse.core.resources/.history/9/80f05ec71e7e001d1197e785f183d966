
class Parent2
{
	void Parent1()
	{
		System.out.println("This is a Parent class");
	}
}
class Child2 extends Parent2
{
	void Child1()
	{
		System.out.println("This is a Parent class");
	}
}
public class Inheritance {

	public static void main(String[] args) {
		Parent2 p=new Parent2();
		p.Parent1();
	}

}
