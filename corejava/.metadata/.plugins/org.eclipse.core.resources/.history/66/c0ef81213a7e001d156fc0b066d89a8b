import java.util.*;
public class IncreaseofSize {

	public static void main(String[] args) {
		
		ArrayList list=new ArrayList();
        list.add("10");
        list.add("20");
        list.add("30");
        list.add("40");
        list.add("50");
        list.add("60");
        System.out.println(list);
        System.out.println("Increasing the capacity of ArrayList numbers to store up");
        list.ensureCapacity(5);
        System.out.println("ArrayList numbers can now store upto 5 elements.");
        System.out.println(list);
     
     
	}

}
