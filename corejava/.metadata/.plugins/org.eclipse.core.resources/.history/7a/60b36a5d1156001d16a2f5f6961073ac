
public class StarPettern4 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int i,j,k;
		for(i=0;i<=5;i++)
	      {
	    	  for(j=5;j>=i;j--)
	    	  {
	    		  System.out.print(" ");
	    	  }
	    	  System.out.println();
	      }
		for(k=0;k<=i;k++)
		{
			System.out.print("*");
		}
		System.out.println();
	}

}
