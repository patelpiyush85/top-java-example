class ComplexNumber
{
	
	int real,imaginary;
	ComplexNumber(){
		
	}
	ComplexNumber(int a,int b)
	{
		a=real;
		b=imaginary;
	}
	void show()
	{
		System.out.println(this.real+"+i"+this.imaginary);
	}
	ComplexNumber  add(ComplexNumber n1)
	{
		ComplexNumber res=new ComplexNumber(0,0);
		res.real=real+n1.real;
		res.imaginary=imaginary+n1.imaginary;
		return res;
	}
}
public class CompleNumberaddition {

	public static void main(String[] args) {
		ComplexNumber c1=new ComplexNumber(40,50);
		ComplexNumber c2=new ComplexNumber(60,70);
		c1.show();
		c2.show();
		ComplexNumber c3=new ComplexNumber();
		c3=c1.add(c2);

	}

}
