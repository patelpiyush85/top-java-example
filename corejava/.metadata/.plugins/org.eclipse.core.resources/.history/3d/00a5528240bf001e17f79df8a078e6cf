class A {
	void show()
	{
		System.out.println("A method call");
	}
}
class B extends A{
	void show()
	{
		System.out.println("B method is call");
	}
}





public class SingleInheritance {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		A ob1=new A();
		ob1.show();

	}

}
